java_model_generation(
    name = "core_gen",
    factory_class = "org.finos.legend.pure.m3.serialization.grammar.m3parser.antlr.M3AntlrParser",
    model_name = "Core",
    tool = ":JavaModelFactoryGenerator",
)

genrule(
    name = "platform_merged_src",
    srcs = [
        ":platform_src.srcjar",
        "src/main/java/org/finos/legend/pure/generated/CoreGen.java",
        "src/main/java/org/finos/legend/pure/generated/Root_meta_pure_functions_lang_KeyValue.java",
        "src/main/java/org/finos/legend/pure/generated/Root_meta_pure_functions_lang_KeyValue_Impl.java",
    ],
    outs = ["platform_merged.srcjar"],
    cmd = """
        cp $(location :platform_src.srcjar) $@
        chmod +w $@
        src_root="legend-pure/legend-pure-runtime/legend-pure-runtime-java-engine-compiled/src/main/java"
        out_abs=$$(pwd)/$@
        pushd $$src_root > /dev/null
        jar -uf $$out_abs org/finos/legend/pure/generated/CoreGen.java org/finos/legend/pure/generated/Root_meta_pure_functions_lang_KeyValue.java org/finos/legend/pure/generated/Root_meta_pure_functions_lang_KeyValue_Impl.java
        popd > /dev/null
    """,
)

partitioned_java_library(
    name = "compiled_engine_generated",
    src = ":platform_merged.srcjar",
    javacopts = [
        "-Xep:IdentityBinaryExpression:OFF",
        "-Xep:IsInstanceIncompatibleType:OFF",
        "-Xep:ComparableType:OFF",
    ],
    visibility = ["//visibility:private"],
    deps = [
        ":compiled_engine_boot",
        ":engine_deps",
    ],
)

java_library(
    name = "legend-pure-runtime-java-engine-compiled",
    visibility = ["//visibility:public"],
    exports = [
        ":compiled_engine_boot",
        ":compiled_engine_generated",
        ":engine_deps",
    ],
)

pure_java_code_gen(
    name = "platform_src",
    exclusions = [
        "org/finos/legend/pure/generated/CoreGen.java",
        "org/finos/legend/pure/generated/Root_meta_pure_functions_lang_KeyValue_Impl.java",
    ],
    metadata_jar = "platform_metadata.jar",
    repository = "platform",
    tool = ":JavaCodeGeneration",
    visibility = ["//visibility:public"],
)

filegroup(
    name = "generate_platform_engine",
    srcs = [
        ":platform_metadata.jar",
        ":platform_src",
    ],
    visibility = ["//visibility:public"],
)

java_import(
    name = "platform_metadata",
    jars = [":platform_metadata.jar"],
    visibility = ["//visibility:public"],
)
